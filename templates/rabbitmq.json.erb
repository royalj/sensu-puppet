{
  "rabbitmq":[
    {
      "host": "<%= @rabbitmq_host[0] %>",
      "password": "<%= @rabbitmq_password %>",
      "vhost": "<%= @rabbitmq_vhost %>",
      "port": <%= @rabbitmq_port %>,
      "user": "<%= @rabbitmq_user %>"
    }<%count = 1 
        while !@rabbitmq_host[count].nil? do -%>,
    {
      "host": "<%= @rabbitmq_host[count] %>",
      "password": "<%= @rabbitmq_password %>",
      "vhost": "<%= @rabbitmq_vhost %>",
      "port": <%= @rabbitmq_port %>,
      "user": "<%= @rabbitmq_user %>"
    }
    <% end -%>
  ]
}
